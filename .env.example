# LLM Provider (openai or holistic_ai)
LLM_PROVIDER=openai

# OpenAI Configuration (when LLM_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=1024

# HolisticAI Bedrock Proxy (when LLM_PROVIDER=holistic_ai)
HOLISTIC_AI_TEAM_ID=your_team_id_here
HOLISTIC_AI_API_TOKEN=your_token_here
HOLISTIC_AI_API_ENDPOINT=https://ctwa92wg1b.execute-api.us-east-1.amazonaws.com/prod/invoke
HOLISTIC_AI_MODEL=us.anthropic.claude-3-5-sonnet-20241022-v2:0

# LangSmith (optional but recommended)
LANGSMITH_API_KEY=your_langsmith_key_here
LANGSMITH_PROJECT=fertility-support-agent
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_TRACING=true

# Application
LOG_LEVEL=INFO
CACHE_ENABLED=true
RATE_LIMIT_PER_MINUTE=60
MAX_MESSAGE_LENGTH=2000
ENVIRONMENT=dev
VERSION=0.1.0
